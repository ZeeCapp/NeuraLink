<Page x:Class="NeuraLink.Graph"
      x:Name="x"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:NeuraLink"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="800"
      Title="NetworkPlain">
    <Grid Background="#FF232323" x:Name="GraphContainer">
        <Grid.RowDefinitions>
            <RowDefinition Height="480"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Canvas Background="#FF232323" x:Name="DataGraph" HorizontalAlignment="Stretch" VerticalAlignment="Top" Height="450" Grid.Row="0">
        </Canvas>
    </Grid>
    
</Page>
